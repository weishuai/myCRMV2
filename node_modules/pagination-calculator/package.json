{
    "name": "pagination-calculator",
    "version": "1.0.0",
    "description": "Utility to calculation pages for pagination",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc -p ./tsconfig.json --outDir dist",
        "build:test": "tsc -p ./tsconfig.test.json",
        "test": "npm run build:test && alsatian \"./test/**/*.test.js\"",
        "lint": "tslint -c tslint.json \"./{src,test}/**/*.{ts,tsx}\"",
        "review": "npm run lint && npm run test"
    },
    "keywords": [
        "pagination"
    ],
    "author": "Henry Ing-Simmons (https://github.com/hisuwh)",
    "license": "MIT",
    "devDependencies": {
        "alsatian": "2.1.0",
        "tslint": "^5.1.0",
        "typescript": "^2.2.2"
    }
}
